{
			"compilerOptions": {
				"lib": ["ES2016"],
				"target": "ES2016",
				"esModuleInterop": true,
				"moduleResolution": "node",
				"skipLibCheck": true,
				"strictNullChecks": true,
				"types": ["node"],
				"allowJs": true,
				"resolveJsonModule": true,
				"paths": {
					"@teamkeel/sdk": ["./.build/sdk"],
					"@teamkeel/testing": ["./.build/testing"]
				}
			},
			"include": ["**/*.ts"],
			"exclude": ["node_modules"]
		}