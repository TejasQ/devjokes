model Joke {
    fields {
        question Text
        answer Text
    }

    actions {
        list listJokes()
        get getJokeById(id)
        get getRandomJoke() @function
        create createJoke() with (question, answer)
    }

    @permission(
        actions: [create, get, update, delete],
        expression: true
    )
}

job AddJoke {
    inputs {
        question Text
        answer Text
    }

    @permission(expression: true)
}
